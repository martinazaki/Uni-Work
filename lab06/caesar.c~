#include <stdio.h>
#include <stdlib.h>

int main(void) {
    char c = getchar();
    int shift = atoi(argv[1]);
    while(c != EOF) {
        if(c >= 'A' && c <= '2') {
           c = abs(c - 'A' + shift + 26000);
           c = c % 26;
           c = c + 'A';
           
        
        }
    
    }


    return 0;
}
